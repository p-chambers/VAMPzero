<cpacs>
	<toolspecific>
		<vampZero>
			<tool>
				<name>VAMPzero</name>
				<version>0.6</version>
			</tool>
			<toolSettings>
				<!-- 
					 <deviationAmplitude> 	->  the random deviation factor is multiplied by this factor
												choose 0.0 to deactivate the deviation 
					 
					 <extendedOutput> 		-> 	true|false
					 							to de/activate plotting and creation of mind maps
					 							default value is true
					 							deactivation will reduce execution time
					 
					 <calcSensitivities>	->  true|false
					 							switch to turn on the calculation of sensitivities (experimental)
					 
					 <notConvergingAction> 	->  exit|copyInput|errorXML
					 							choose the action to be performed when a calculation is not converging
					 							default is exit 
				-->
				<deviationAmplitude>0.0</deviationAmplitude>
				<extendedOutput>true</extendedOutput>
				<calcSensitivities>false</calcSensitivities>
				<notConvergingAction>exit</notConvergingAction>
			</toolSettings>
			<aircraftModelUID>SBW</aircraftModelUID>
			<components>
				<component>
					<name>Atmosphere</name>
					<disciplines>
						<discipline>
							<name>Airport</name>
							<parameters>
								<parameter>
									<name>deltaT</name>
									<value>0.</value>
									<factor>1.0</factor>
								</parameter>
							</parameters>
						</discipline>
					</disciplines>
				</component>
				<component>
					<name>Aircraft</name>
					<disciplines>                   
						<discipline>
							<name>Performance</name>
							<parameters>
								<parameter>
									<name>desRange</name>
									<value>5556000.</value>
									<factor>1.0</factor>
								</parameter>
								<parameter>
									<name>altCR</name>
									<value>10000.</value>
									<factor>1.0</factor>
								</parameter>
								<parameter>
									<name>machCR</name>
									<value>0.75</value>
									<factor>1.0</factor>
								</parameter>
							</parameters>
						</discipline>
					</disciplines>
				</component>
				<component>
					<name>Payload</name>
					<disciplines>
						<discipline>
							<name>Mass</name>
							<parameters>
								<parameter>
									<name>paxSeats</name>
									<value>180</value>
									<factor>1.0</factor>
								</parameter>
							</parameters>
						</discipline>
					</disciplines>
				</component>
				<component>
					<name>Wing</name>
					<disciplines>
						<discipline>
							<name>Geometry</name>
							<parameters>
								<parameter>
									<name>taperRatio</name>
									<value>0.235</value>
									<factor>1.0</factor>
								</parameter>
								<parameter>
									<name>aspectRatio</name>
									<value>12.</value>
									<factor>1.0</factor>
								</parameter>
								<parameter>
									<name>phi25</name>
									<value>16.</value>
									<factor>1.0</factor>
								</parameter>
								<parameter>
									<name>refArea</name>
									<value>160</value>
									<factor>1.0</factor>
								</parameter>
								<parameter>
									<name>tcAVG</name>
									<value>.1</value>
									<factor>1.0</factor>
								</parameter>
								<parameter>
									<name>zRoot</name>
									<value>2.</value>
									<factor>1.0</factor>
								</parameter>
							</parameters>
						</discipline>
                    </disciplines>
				</component>
				<component>
					<name>Htp</name>
					<disciplines>
						<discipline>
							<name>Geometry</name>
							<parameters>
								<parameter>
									<name>aspectRatio</name>
									<value>5.3</value>
									<factor>1.0</factor>
								</parameter>
								<parameter>
									<name>taperRatio</name>
									<value>0.378</value>
									<factor>1.0</factor>
								</parameter>
							</parameters>
						</discipline>
					</disciplines>
				</component>
				<component>
					<name>Vtp</name>
					<disciplines>
						<discipline>
							<name>Geometry</name>
							<parameters>
								<parameter>
									<name>aspectRatio</name>
									<value>1.6</value>
									<factor>1.0</factor>
								</parameter>
								<parameter>
									<name>taperRatio</name>
									<value>0.397</value>
									<factor>1.0</factor>
								</parameter>
							</parameters>
						</discipline>
					</disciplines>
				</component>
				<component>
					<name>Engine</name>
					<disciplines>
						<discipline>
							<name>Propulsion</name>
							<parameters>
								<parameter>
									<name>nEngine</name>
									<value>2</value>
									<factor>1.0</factor>
								</parameter>
								<parameter>
									<name>bypassRatio</name>
									<value>6.9</value>
									<factor>1.0</factor>
								</parameter>
							</parameters>
						</discipline>
					</disciplines>
				</component>				
		</components>
		</vampZero>
	</toolspecific>
</cpacs>
